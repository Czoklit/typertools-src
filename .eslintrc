{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-prototype-builtins": 0,
        "require-atomic-updates": 0,
        "react/display-name": 0,
        "react-hooks/rules-of-hooks": "error"
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react", "react-hooks"]
}